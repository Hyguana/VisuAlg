algoritmo "Operadores Lógicos"
// Função :
// Autor :
// Data : 26/01/2020
// Seção de Declarações 
var
   a,b,c:inteiro
inicio
// Seção de Comandos 
   a<-2
   b<-3
   c<-5
   
   escreva("Verdadeiro E Falso: ",verdadeiro e falso)
   escreval
   escreva("A=B OU C>A: ",(a=b)ou(c>a))
   escreval
   escreva("Negativo de A=B OU C>A: ",nao((a=b)ou(c>a)))
   escreval
   escreva
   
fimalgoritmo
